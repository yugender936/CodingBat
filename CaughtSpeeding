public int caughtSpeeding(int speed, boolean isBirthday) {
  int i = 0;
  if(isBirthday){
    if(speed >= 66 && speed <= 85){
      i = 1;
    }
    if(speed >= 86){
      i = 2;
    }
  }
  else{
    if(speed >= 61 && speed <= 80){
      i = 1;
    }
    if(speed >= 81){
      i = 2;
    }
    
  }
  return i;
}
